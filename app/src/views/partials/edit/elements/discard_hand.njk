<label for='{{key}}_value' class='title'>Discard hands</label>
<select name='[section][{{section_index}}][elements][{{element_index}}][hand_count]' id='{{key}}_value'>
    {% for index in range(1, 9) %}
    <option{% if element.hand_count_range.max == index %} selected{% endif %}>{{index}}</option>
    {% endfor %}
</select>
