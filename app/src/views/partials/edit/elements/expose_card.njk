<label for='{{key}}_value' class='title'>Expose cards</label>
<select name='[section][{{sectionIndex}}][elements][{{elementIndex}}][card_count]' id='{{key}}_value'>
    {% for index in range(1, 9) %}
    <option{% if element.card_count_range.min == index %} selected{% endif %}>{{index}}</option>
    {% endfor %}
</select>
