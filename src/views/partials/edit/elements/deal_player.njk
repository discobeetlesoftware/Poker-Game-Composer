<label for='{{key}}_value' class='title'>Down cards</label>
<select name='[section][{{section_index}}][elements][{{element_index}}][card_count]' id='{{key}}_value'>
    {% for index in range(1, 9) %}
    <option{% if element.card_count == index %} selected{% endif %}>{{index}}</option>
    {% endfor %}
</select>
<label for='{{key}}_face' class='title'>Is face up</label>
<input type='checkbox' id='{{key}}_face' name='[section][{{section_index}}][elements][{{element_index}}][is_face_up]'{% if element.is_face_up %} checked {% endif %}/>