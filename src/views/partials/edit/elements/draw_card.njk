<label for='{{key}}_value' class='title'>Draw cards</label>
<select name='[section][{{section_index}}][elements][{{element_index}}][card_count]' id='{{key}}_value'>
    {% for index in range(1, 9) %}
    <option{% if element.card_count_range.max == index %} selected{% endif %}>{{index}}</option>
    {% endfor %}
</select>
